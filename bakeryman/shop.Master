<%@ Master Language="C#" AutoEventWireup="true" CodeBehind="shop.Master.cs" Inherits="bakeryman.Site3" %>

<!DOCTYPE html>

<html xmlns="http://www.w3.org/1999/xhtml">
<head runat="server">
    <title></title>
    <style type="text/css">
